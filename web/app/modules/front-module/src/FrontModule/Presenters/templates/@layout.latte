{**
 * @param string   $basePath web base path
 * @param array    $flashes  flash messages
 *}
<!DOCTYPE html>
<html lang="cs">
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta content="cs" http-equiv="Content-language">
	<meta name="description" content="">
	<meta name="keywords" content="">
	<meta content="Pixman s.r.o." name="author">
	<link rel="shortcut icon" href="{$basePath}/favicon.png">
	<link href="{$basePath}/resources/cmsModule/select2/docs/themes/site/images/apple-touch-icon.png" rel="apple-touch-icon">

	<title>{ifset title}{include title|striptags} | {/ifset}Cms - Pixatori</title>

	<link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Montserrat:300,400,500,700" rel="stylesheet">

	{*================== BEGIN BASE CSS STYLE ==================*}
	<script type="application/javascript">var dataLayer = dataLayer || []; // Google Tag Manager</script>

{*	{var $production = true}*}

    {if $production}
		{control css}

	{else}
		{*<!-- build:css {$basePath}/css/frontScreen.min.css -->*}
		<link href="{$basePath}/resources/frontModule/bootstrap/css/bootstrap.min.css" rel="stylesheet">
		<link href="{$basePath}/resources/frontModule/animate.css/animate.min.css" rel="stylesheet">
		<link href="{$basePath}/resources/frontModule/owlcarousel/assets/owl.carousel.css" rel="stylesheet" type="text/css" />
		<link href="{$basePath}/resources/frontModule/owlcarousel/assets/owl.theme.default.css" rel="stylesheet" type="text/css" />
		<link href="{$basePath}/resources/frontModule/lightbox2/dist/css/lightbox.css" rel="stylesheet" type="text/css">
		<link href="{$basePath}/css/front.css" rel="stylesheet" type="text/css" media="screen">
		{*<!-- endbuild -->*}
	{/if}
	<link href="{$basePath}/resources/frontModule/font-awesome/css/font-awesome.min.css" rel="stylesheet">
	<link href="{$basePath}/resources/frontModule/ionicons/css/ionicons.min.css" rel="stylesheet">

	{*================== BEGIN BASE JS ==================*}
	<script type="application/javascript" src="{$basePath}/resources/frontModule/jquery/dist/jquery.js"></script>
	{*================== END BASE JS ==================*}
	{block head}{/block}
</head>

<body>

	<header id="header">
		<div class="container-fluid">

			<div id="logo" class="pull-left">
				<h1><a n:href="Homepage:#intro" class="scrollto">CMS Pixatori</a></h1>
				{*<a href="#intro"><img src="img/logo.png" alt="" title="" /></a>*}
			</div>

			<nav id="nav-menu-container">
				<ul class="nav-menu">
					<li n:class="$presenter->isLinkCurrent('Homepage:*') ? 'menu-active'"><a n:href="Homepage:#intro">Home</a></li>
					{*<li><a href="#about">About Us</a></li>*}
					<li><a n:href="Homepage:#services">Služby</a></li>
					<li><a n:href="Homepage:#portfolio">Portfolio</a></li>
					<li n:class="$presenter->isLinkCurrent('Api:*') ? 'menu-active'"><a n:href="Api:example">Api v příkladech</a></li>
					<li n:class="$presenter->isLinkCurrent(':Cms:Campaign:*') ? 'menu-active'"><a n:href=":Cms:Campaign:">Admin</a></li>
				</ul>

			</nav>
		</div>
	</header>

	<main id="main">
		{snippet content}
			{include content}
		{/snippet}
	</main>

	{include '#footer.latte'}
	{*{control flashMessage}*}

	{block scripts}
		{*================== BEGIN BASE JS ==================*}
		{if $production && true == false}
			<script type="application/javascript" data-pf-placeholder="scripts" src="{$basePath}/js/index.min.js?rel=1531896897707"></script>

			<script type="application/javascript" src="{$basePath}/resources/frontModule/nette.ajax.js/nette.ajax.js"></script>
			<script type="application/javascript" src="{$basePath}/resources/frontModule/nette-live-form-validation/live-form-validation.js"></script>
			<script type="application/javascript" src="{$basePath}/resources/frontModule/nette.ajax.js/extensions/diagnostics.dumps.ajax.js"></script>
			<script type="application/javascript" src="{$basePath}/resources/frontModule/nette.ajax.js/extensions/fidRemove.ajax.js"></script>
			<script type="application/javascript" src="{$basePath}/resources/frontModule/nette.ajax.js/extensions/confirm.ajax.js"></script>



			<script type="application/javascript" src="{$basePath}/js/nette.nprogress.js"></script>
			<script type="application/javascript" src="{$basePath}/resources/frontModule/nprogress/nprogress.js"></script>
		{else}
			{*<!-- build:js {$basePath}/js/index.min.js -->*}
			<script type="application/javascript" src="{$basePath}/resources/frontModule/nette.ajax.js/nette.ajax.js"></script>
			<script type="application/javascript" src="{$basePath}/resources/frontModule/nette-live-form-validation/live-form-validation.js"></script>
			<script type="application/javascript" src="{$basePath}/resources/frontModule/nette.ajax.js/extensions/diagnostics.dumps.ajax.js"></script>
			<script type="application/javascript" src="{$basePath}/resources/frontModule/nette.ajax.js/extensions/fidRemove.ajax.js"></script>
			<script type="application/javascript" src="{$basePath}/resources/frontModule/nette.ajax.js/extensions/confirm.ajax.js"></script>

			<script src="{$basePath}/resources/frontModule/bootstrap/js/bootstrap.min.js"></script>
			<script src="{$basePath}/resources/frontModule/easing/easing.min.js"></script>
			<script src="{$basePath}/resources/frontModule/superfish/hoverIntent.js"></script>
			<script src="{$basePath}/resources/frontModule/superfish/superfish.min.js"></script>
			<script src="{$basePath}/resources/frontModule/wow/wow.min.js"></script>
			<script src="{$basePath}/resources/frontModule/waypoints/waypoints.min.js"></script>
			<script src="{$basePath}/resources/frontModule/counterup/counterup.min.js"></script>
			<script src="{$basePath}/resources/frontModule/owlcarousel/owl.carousel.min.js"></script>
			<script src="{$basePath}/resources/frontModule/isotope/isotope.pkgd.min.js"></script>
			<script src="{$basePath}/resources/frontModule/lightbox2/dist/js/lightbox.js"></script>
			<script src="{$basePath}/resources/frontModule/touchSwipe/jquery.touchSwipe.min.js"></script>

			<script src="{$basePath}/js/front.js"></script>


			<script type="application/javascript" src="{$basePath}/js/nette.nprogress.js"></script>
			<script type="application/javascript" src="{$basePath}/resources/frontModule/nprogress/nprogress.js"></script>
			{*<!-- endbuild -->*}

		{/if}
		{*================== END BASE JS ==================*}
	{/block}
	{block extraScripts}{/block}

	<script data-pf-placeholder="scripts">
		(function($, undefined) {
			$(function () {
				$.nette.init();
			});
		})(jQuery); //jNIV  test
	</script>

</body>
</html>
